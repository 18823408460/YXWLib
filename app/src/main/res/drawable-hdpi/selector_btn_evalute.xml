<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape >
            <solid android:color="@color/evalute_btn_bg2"/>
            <corners android:radius="2dp"/>
            <stroke android:width="2dp"
                android:color="@color/evalute_btn_frame2"/>
        </shape>
    </item>
    <item>
        <shape >
            <solid android:color="@color/evalute_btn_bg"/>
            <corners android:radius="2dp"/>
            <stroke android:width="2dp"
                android:color="@color/evalute_btn_frame"/>
        </shape>
    </item>

</selector>
